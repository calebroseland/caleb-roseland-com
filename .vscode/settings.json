{
  "prettier.semi": false,
  "eslint.enable": true,
  "eslint.run": "onType",
  "eslint.validate": [
    {
      "language": "vue",
      "autoFix": true
    },
    {
      "language": "html",
      "autoFix": true
    },
    {
      "language": "javascript",
      "autoFix": true
    },
    {
      "language": "javascriptreact",
      "autoFix": true
    }
  ],
  "eslint.alwaysShowStatus": true,
  "eslint.autoFixOnSave": true,
  "vetur.format.defaultFormatter.js": "prettier-eslint",
  "vetur.format.defaultFormatter.css": "prettier",
  "workbench.colorCustomizations": {
    "activityBar.background": "#093430",
    "titleBar.activeBackground": "#0D4844",
    "titleBar.activeForeground": "#F0FCFB"
  },
  "spellright.language": [
    "en"
  ],
  "spellright.documentTypes": [
    "markdown",
    "latex",
    "plaintext",
    "json"
  ]
}
